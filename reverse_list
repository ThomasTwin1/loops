# Reverse Method
squares = []
for value in range(1,11):
    squares.append(value-2)

squares.reverse()
print(squares) #output: [8, 7, 6, 5, 4, 3, 2, 1, 0, -1]

# Reverse list using slicing
squares = []
for value in range(1,11):
    squares.append(value-2)

reverse_squares = squares[::-1]
print(reverse_squares) #output: [8, 7, 6, 5, 4, 3, 2, 1, 0, -1]
